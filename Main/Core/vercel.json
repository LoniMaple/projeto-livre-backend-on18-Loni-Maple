{
    "version": 2,
    "builds": [
        {
            "src": "src/app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/app.js",
            "methods": [
                "GET","POST","DELETE","PATCH","PUT","OPTIONS"
            ]
        }
    ]
}